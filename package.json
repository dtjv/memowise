{
  "name": "memowise",
  "version": "1.0.0",
  "description": "A flashcard app using React + Redux",
  "engines": {
    "node": "7"
  },
  "scripts": {
    "clean": "rimraf build",
    "lint": "eslint *.js bin src test --color",
    "build": "npm run clean && webpack --progress --colors",
    "start": "node src/server/server.js",
    "e2e": "npm run import && cross-env NODE_PATH=src codeceptjs run --steps",
    "test": "npm run lint && cross-env NODE_PATH=src nyc mocha test/client test/server",
    "selenium": "selenium-standalone install && selenium-standalone start",
    "import": "./bin/deck-import-cli.js -a",
    "postinstall": "npm run build",
    "coverage": "nyc report --reporter=html && open ./coverage/index.html",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "codeceptjs": "2.6.6",
    "codecov": "3.7.1",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "lodash.range": "3.2.0",
    "mocha": "^8.0.1",
    "nock": "^13.0.2",
    "nyc": "^15.1.0",
    "react-addons-test-utils": "15.6.2",
    "redux-mock-store": "^1.5.4",
    "selenium-standalone": "6.17.0",
    "supertest": "^4.0.2",
    "webdriverio": "6.2.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "css-loader": "^3.6.0",
    "dotenv-safe": "8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "is-empty-object": "1.1.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.19",
    "meow": "7.0.1",
    "moment": "^2.27.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.24",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "3.0.2",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "uglify-js": "3.10.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "unique-random-array": "2.0.0",
    "webpack": "^4.43.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "stage-2"
    ]
  },
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "private": true
}
